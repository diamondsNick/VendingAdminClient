<Window x:Class="AdminClient.Views.MainInterfaceWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AdminClient.Views"
        mc:Ignorable="d"
        Title="MainInterfaceWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="17*" />
            <ColumnDefinition Width="63*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="35*"/>
            <RowDefinition Height="62*"/>
            <RowDefinition Height="337*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="1" Grid.RowSpan="2" Background="#1d2127" Margin="0,1,0,0"></Grid>
        <Grid Grid.Column="1" Grid.Row="1" Background="#1d2127"/>
        <Grid Grid.Column="1" Grid.Row="2" Background="#d9d9d9"></Grid>




    </Grid>
</Window>
